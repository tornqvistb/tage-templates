<?xml version="1.0"?>
<!DOCTYPE layout-templates PUBLIC "-//Liferay//DTD Layout Templates 6.2.0//EN" "http://www.liferay.com/dtd/liferay-layout-templates_6_2_0.dtd">

<layout-templates>
	<custom>
		<layout-template id="tagestartpagetemplate"
			name="TAGE Startpage Template">
			<template-path>/tagestartpagetemplate.tpl</template-path>
			<wap-template-path>
				/tagestartpagetemplate.wap.tpl
			</wap-template-path>
			<thumbnail-path>
				/tagestartpagetemplate.png
			</thumbnail-path>
		</layout-template>
		<layout-template id="tagepagetemplate"
			name="TAGE Tagepage Template">
			<template-path>/tagepagetemplate.tpl</template-path>
			<wap-template-path>
				/tagepagetemplate.wap.tpl
			</wap-template-path>
			<thumbnail-path>
				/tagepagetemplate.png
			</thumbnail-path>
		</layout-template>
		<layout-template id="tage2colspagetemplate"
			name="TAGE 2 columns Template">
			<template-path>/tage2colspagetemplate.tpl</template-path>
			<wap-template-path>
				/tage2colspagetemplate.wap.tpl
			</wap-template-path>
			<thumbnail-path>
				/tage2colspagetemplate.png
			</thumbnail-path>
		</layout-template>
		<layout-template id="tage1colpagetemplate"
			name="TAGE 1 column Template">
			<template-path>/tage1colpagetemplate.tpl</template-path>
			<wap-template-path>
				/tage1colpagetemplate.wap.tpl
			</wap-template-path>
			<thumbnail-path>
				/tage1colpagetemplate.png
			</thumbnail-path>
		</layout-template>
		<layout-template id="tagereglerpagetemplate"
			name="TAGE Reglerpage Template">
			<template-path>/tagereglerpagetemplate.tpl</template-path>
			<wap-template-path>
				/tagereglerpagetemplate.wap.tpl
			</wap-template-path>
			<thumbnail-path>
				/tagereglerpagetemplate.png
			</thumbnail-path>
		</layout-template>
	</custom>
</layout-templates>